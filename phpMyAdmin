1)
SELECT * 
FROM `students` 
WHERE year(`date_of_birth`) = '1990';

2)
SELECT * 
FROM `courses` 
WHERE `cfu` > 10;

3)
SELECT * 
FROM `students` 
WHERE year(`date_of_birth`) < 1993;

4)
SELECT * 
FROM `courses` 
WHERE `period` = 'I semestre' 
AND `year` = 1;

5)
SELECT * 
FROM `exams` 
WHERE `date` = '20-06-20' 
AND hour(`hour`) >= 14;

6)
SELECT * 
FROM `degrees` 
WHERE `level` = 'magistrale';

7)
SELECT * 
FROM `departments`;

8)
SELECT * 
FROM `teachers` 
WHERE `phone` is null;